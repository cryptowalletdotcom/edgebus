			"name": "Maxim Anurin",
			"email": "maxim.anurin@zxteam.org",
			"url": "http://www.anurin.name"
		},
		{
			"name": "Serhii Zghama",
			"email": "serhii.zghama@zxteam.org"
		}
	],
	"license": "MIT",
	"devDependencies": {
		"@types/lodash": "^4.14.149",
		"@types/node": "^11.15.3",
		"@types/uuid": "^3.4.6",
		"@zxnode/build": "^0.0.34"
	},
	"dependencies": {
		"@zxteam/cancellation": "^6.0.40",
		"@zxteam/channels": "^6.0.53",
		"@zxteam/configuration": "^6.0.56",
		"@zxteam/contract": "^6.0.56",
		"@zxteam/disposable": "^6.0.56",
		"@zxteam/ensure": "^6.0.41",
		"@zxteam/errors": "^6.0.41",
		"@zxteam/hosting": "^6.0.42",
		"@zxteam/launcher": "^6.0.26",
		"@zxteam/logger": "^6.0.40",
		"@zxteam/sql-postgres": "^6.0.53",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"uuid": "^3.4.0"
	},
	"engines": {
		"node": ">=11"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git@github.com:zxteamorg/node.notifier.service.git"
	},
	"bugs": {
		"url": "https://github.com/zxteamorg/node.notifier.service/issues"
	},
	"homepage": "https://github.com/zxteamorg/node.notifier.service",
	"scripts": {
		"clean": "zxbuild clean",
		"build": "zxbuild clean && zxbuild dist && cp README.md package-lock.json .dist/ && cp -a bin .dist/ && cp -a database .dist/",
		"start": "node bin/notifier-service.js --config=notifier-service.config",
		"test": "true",
		"test:next": "nyc --exclude submodules --exclude test --extension=\".ts\" --reporter=lcov --reporter=text --reporter=text-summary mocha \"test/**/*.test.js\" --recursive mocha --timeout 10000",
		"test:local": "env TEST_DB_URL=\"postgres://\" mocha --require source-map-support/register --recursive \"test/**/*.test.js\"",
		"migrate-database:local": "env persistentStorageMigration.url=\"postgres://postgres:@127.0.0.1:5432/emptytestdb?schema=notifier\" node bin/migrate-database.js"
	},
	"private": true
}
